<p class="pageHeader"> All Books</p>

<div>
    <table>
        <tr>
            <th>Title</th>
            <th>Author</th>
            <th>On Loan</th>
        </tr>

        #foreach($book in $books)
        <tr>
            <td>$book.getTitle()</td>
            <td>$book.getAuthor()</td>
            <td>$book.isOnLoan()</td>
            <td></td>
        </tr>
        #end
    </table>




</div>
